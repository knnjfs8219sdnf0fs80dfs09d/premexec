-- Main Tab
local Lock = Window:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

-- Lock Section
local LockSec = Lock:AddSection({
    Name = "Lock"
})

