-- TELEPORT

local Teleport = Window:MakeTab({
	Name = "Teleport",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Teleports = Teleport:AddSection({
	Name = "Teleports"
})

Teleports:AddButton({
	Name = "Bank",
	Callback = function()
        getgenv().game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-437.9585266113281, 38.98053741455078, -283.9601745605469)
  	end    
})

Teleports:AddButton({
