local response = request {
    Url = Webhook_URL,
    Method = "POST",
    Headers = {
        ["Content-Type"] = "application/json"
    },
    Body = HttpService:JSONEncode({
        ["content"] = "",
        ["embeds"] = {{
            ["title"] = "**Your script has been executed**",
            ["description"] = game.Players.LocalPlayer.Name.." ("..game.Players.LocalPlayer.DisplayName..") has executed the script.",
            ["type"] = "rich",
            ["color"] = tonumber(ad1010),
            ["fields"] = {{
                ["name"] = "Hardware ID:",
                ["value"] = game:GetService("RbxAnalyticsService"):GetClientId(),
                ["inline"] = true
            }}
        }}
    })
}
