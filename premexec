-- BLACKLISTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTT

local function banPlayer(player)
    player:Kick("TWR Has banned you from this script, you can buy an unban on our Discord server. discord.gg/R57rsmdAzk")
end

local playerNameToBan = ""
local playerToBan = game.Players:FindFirstChild(playerNameToBan)

if playerToBan then
    banPlayer(playerToBan)
else
    print("Player not found.")
end

-- START

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({
    Name = "TWR (Premium)",
    HidePremium = false,
    SaveConfig = true,
    ConfigFolder = "TWR",
    IntroText = "Made by TWR Team.",
    IntroIcon = "rbxassetid://17460077785"
})

OrionLib:MakeNotification({
    Name = "TWR Premium",
    Content = "Successfully loaded! V1.3",
    Image = "rbxassetid://4483345998",
    Time = 10
})

