local Extras = Window:MakeTab({
	Name = "Visuals",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Extrasss = Extras:AddSection({
	Name = "Knife Pull out Anims (need to reclick after respawn/stomp)"
})

Extrasss:AddButton({
	Name = "Minecraft/Pixel Sword Anim",
	Callback = function()
        local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local backpack = player:WaitForChild("Backpack")
local knifeName = "[Knife]"
local animationId = "rbxassetid://14496531574"

local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
local originalCFrame = humanoidRootPart.CFrame

local function playKnifeAnimation()
    local animation = Instance.new("Animation")
    animation.AnimationId = animationId

    local humanoid = character:FindFirstChildOfClass("Humanoid")
    if humanoid then
        local animationTrack = humanoid:LoadAnimation(animation)
        animationTrack:Play()
    end
end

local function equipKnife(knife)
    knife.Parent = character
    knife.Equipped:Connect(playKnifeAnimation)
    playKnifeAnimation() 
end

local knife = backpack:FindFirstChild(knifeName) or character:FindFirstChild(knifeName)

if knife then
    if knife.Parent == backpack then
        equipKnife(knife)
    else
        knife.Equipped:Connect(playKnifeAnimation)
    end
else
    local shopItem = game.Workspace.Ignored.Shop[knifeName .. " - $159"]
    humanoidRootPart.CFrame = shopItem.Head.CFrame + Vector3.new(0, 3, 0)

    if (humanoidRootPart.Position - shopItem.Head.Position).Magnitude <= 50 then
        wait(0.2)
        fireclickdetector(shopItem:FindFirstChild("ClickDetector"), 4)
        knife = backpack:WaitForChild(knifeName)
        equipKnife(knife)

        humanoidRootPart.CFrame = originalCFrame
    end
end

    end})

    Extrasss:AddButton({
        Name = "Soon more anims..",
        Callback = function()
            
        end})

