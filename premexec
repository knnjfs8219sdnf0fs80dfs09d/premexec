-- TOGGLES

local Toggles = Window:MakeTab({
	Name = "Toggles",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local ToggleSec = Toggles:AddSection({
	Name = "Toggles"
})

ToggleSec:AddToggle({
    Name = "Anti-Stomp",
    Default = false,
    Callback = function(Value)
        if Value then
            Antistomp = true
            while Antistomp do
                wait(0.1)
                pcall(function()
                    local localPlayer = game:GetService("Players").LocalPlayer
                    local character = localPlayer.Character
                    if character and character:FindFirstChild("BodyEffects") then
                        local koValue = character.BodyEffects:FindFirstChild("K.O")
                        if koValue and koValue.Value == true then
                            for _, part in ipairs(character:GetDescendants()) do
                                if part:IsA("BasePart") then
                                    part:Destroy()
                                end
                            end
                        end
                    end
                end)
            end
        else
            Antistomp = false
        end
    end
})

ToggleSec:AddToggle({
    Name = "Anti-Slow",
    Default = false,
    Callback = function(Value)
        if Value then
            Antislow = true
            function AntislowLoop()
                while Antislow do
                    wait()
                    local char = game.Players.LocalPlayer.Character
                    if char and char.Parent then -- Check if the character still exists
                        if char.BodyEffects.Movement:FindFirstChild("NoWalkSpeed") then
                            char.BodyEffects.Movement:FindFirstChild("NoWalkSpeed"):Destroy()
                        end
                        if char.BodyEffects.Movement:FindFirstChild("ReduceWalk") then
                            char.BodyEffects.Movement:FindFirstChild("ReduceWalk"):Destroy()
                        end
                        if char.BodyEffects.Movement:FindFirstChild("NoJumping") then
                            char.BodyEffects.Movement:FindFirstChild("NoJumping"):Destroy()
                        end
                        if char.BodyEffects.Reload.Value == true then
                            char.BodyEffects.Reload.Value = false
                        end
                    else
                        Antislow = false
                    end
                end
            end

            AntislowLoop()
            
        else
            Antislow = false
        end
    end
})

ToggleSec:AddToggle({
    Name = "Auto-Stomp",
    Default = false,
    Callback = function(Value)
        if Value then
            Autostomp = true
            while Autostomp do
                wait()
                game.ReplicatedStorage.MainEvent:FireServer("Stomp")
            end
        else
            Autostomp = false
        end
    end
})

ToggleSec:AddToggle({
    Name = "Anti-Bag",
    Default = false,
    Callback = function(Value)
        if Value then
            Antibag = true
            while Antibag do
                wait()
                pcall(function()
                    local character = game.Players.LocalPlayer.Character
                    if character and character:FindFirstChild("Christmas_Sock") then
                        character:FindFirstChild("Christmas_Sock"):Destroy()
                    end
                end)
            end
        else
            Antibag = false
        end
    end
})

ToggleSec:AddToggle({
    Name = "Auto-Armor",
    Default = false,
    Callback = function(Value)
        if Value then
            Autoarmor = true
            while Autoarmor do
                wait()
                
                function buyarmor()
                    local plr = game:GetService("Players").LocalPlayer
                    local gunName = "[Medium Armor] - $1066"
                    local ITEM = game.Workspace.Ignored.Shop[gunName]
                    
                    if ITEM then
                        local character = plr.Character
                        local datplace = character.HumanoidRootPart.Position
                        
                        character.HumanoidRootPart.CFrame = ITEM.Head.CFrame + Vector3.new(0, 3, 0)
                        wait(0.5)
                        fireclickdetector(ITEM.ClickDetector)
                        fireclickdetector(ITEM.ClickDetector)
                        character.HumanoidRootPart.CFrame = CFrame.new(datplace)
                    else
                        print("Armor item not found in shop.")
                    end
                end
                
                local character = game.Players.LocalPlayer.Character
                if character and character.BodyEffects and character.BodyEffects.Armor and character.BodyEffects.Armor.Value == 0 then
                    buyarmor()
                end
            end
        else
            Autoarmor = false
        end
    end
})


ToggleSec:AddToggle({
    Name = "Anti-Grab",
    Default = false,
    Callback = function(Value)
        if Value then
            Antigrab = true
            while Antigrab do
                wait()
                local player = game.Players.LocalPlayer
                local character = player.Character
                if character then
                    local GC = character:FindFirstChild("GRABBING_CONSTRAINT")
                    if GC then
                        GC:Destroy()
                        wait(0.04)
                        if character:FindFirstChildWhichIsA('Humanoid') then
                            character:FindFirstChildWhichIsA('Humanoid').Sit = true
                        end
                    end
                end
            end
        else
            Antigrab = false
        end
    end
})

ToggleSec:AddToggle({
    Name = "Anti-Fling",
    Default = false,
    Callback = function(Value)
        if Value then
            game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
        else
            game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
        end
    end
})

ToggleSec:AddToggle({
    Name = "Auto-Reload",
    Default = false,
    Callback = function(Value)
        if Value then
            Reload = true
            while Reload do
                wait()
                local tool = game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool")
                if tool and tool:FindFirstChild("Ammo") then
                    if tool.Ammo.Value <= 0 then
                        game.ReplicatedStorage.MainEvent:FireServer("Reload", tool)
                    end
                end
            end
        else
            Reload = false
        end
    end
})

local ToggleSecCash = Toggles:AddSection({
	Name = "Cash Stuff"
})

ToggleSecCash:AddToggle({
    Name = "Auto-Drop",
    Default = false,
    Callback = function(Value)
        if Value then
            Autodrop = true
            while Autodrop do
                wait()
                local args = {"DropMoney", "10000"}
                game:GetService("ReplicatedStorage").MainEvent:FireServer(unpack(args))
            end
        else
            Autodrop = false
        end
    end
})

ToggleSecCash:AddToggle({
    Name = "Cash Aura",
    Default = false,
    Callback = function(Value)
        if Value then
            MoneyauraToggle = true
            while MoneyauraToggle do
                wait()
                for _, v in pairs(game:GetService('Workspace').Ignored.Drop:GetChildren()) do
                    if v:IsA('Part') then
                        local playerPosition = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
                        if (v.Position - playerPosition).Magnitude <= 12 then
                            fireclickdetector(v:FindFirstChild('ClickDetector'))
                        end
                    end
                end
            end
        else
            MoneyauraToggle = false
        end
    end
})

